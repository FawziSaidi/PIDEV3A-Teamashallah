<script>
document.addEventListener('DOMContentLoaded', function() {
    var roleSelect = document.getElementById('registration_form_role');
    var commonFields = document.getElementById('common-fields');
    var studentFields = document.getElementById('student-fields');
    var teacherFields = document.getElementById('teacher-fields');
    var hrmClubFields = document.getElementById('hrm-club-fields');
    var hrmStageFields = document.getElementById('hrm-stage-fields');
    var termsAndSubmit = document.getElementById('terms-and-submit');

    roleSelect.addEventListener('change', function() {
        commonFields.style.display = 'block';
        termsAndSubmit.style.display = 'block';
        studentFields.style.display = 'none';
        teacherFields.style.display = 'none';
        hrmClubFields.style.display = 'none';
        hrmStageFields.style.display = 'none';

        switch(this.value) {
            case 'ROLE_STUDENT':
                studentFields.style.display = 'block';
                break;
            case 'ROLE_TEACHER':
                teacherFields.style.display = 'block';
                break;
            case 'ROLE_HRM_CLUB':
                hrmClubFields.style.display = 'block';
                break;
            case 'ROLE_HRM_STAGE':
                hrmStageFields.style.display = 'block';
                break;
        }
    });

    // Live Validation
    var form = document.getElementById('registration-form');
    var inputs = form.querySelectorAll('input, textarea, select');

    inputs.forEach(function(input) {
        input.addEventListener('input', function() {
            validateField(input);
        });
    });

    function validateField(input) {
        var errorElement = input.nextElementSibling;
        if (!errorElement || !errorElement.classList.contains('invalid-feedback')) {
            errorElement = document.createElement('div');
            errorElement.classList.add('invalid-feedback');
            input.parentNode.insertBefore(errorElement, input.nextSibling);
        }

        var validationResults = getValidationResults(input);

        if (validationResults.isValid) {
            input.classList.remove('is-invalid');
            input.classList.add('is-valid');
            errorElement.innerHTML = '';
        } else {
            input.classList.remove('is-valid');
            input.classList.add('is-invalid');
            errorElement.innerHTML = validationResults.messages.join('<br>');
        }
    }

    function getValidationResults(input) {
        var isValid = true;
        var messages = [];

        if (!input.validity.valid) {
            isValid = false;
            messages.push(input.validationMessage);
        }

        // Custom validation rules based on Symfony Assert constraints
        switch (input.id) {
            case 'registration_form_email':
                if (!isValidEmail(input.value)) {
                    isValid = false;
                    messages.push('Please enter a valid email address.');
                }
                break;
            case 'registration_form_plainPassword':
                if (input.value.length < 6) {
                    isValid = false;
                    messages.push('Password must be at least 6 characters long.');
                }
                break;
            case 'registration_form_phone_number':
                if (!isValidPhoneNumber(input.value)) {
                    isValid = false;
                    messages.push('Please enter a valid phone number (format: +216xxxxxxxx).');
                }
                break;
        }

        return { isValid: isValid, messages: messages };
    }

    function isValidEmail(email) {
        // Email REGEX
        return /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email);
    }

    function isValidPhoneNumber(phone) {
        // Phone number validation for tounis
        return /^\+216\d{8}$/.test(phone);
    }

    // Prevent form submission if there are invalid fields
    form.addEventListener('submit', function(event) {
        var invalidInputs = form.querySelectorAll('.is-invalid');
        if (invalidInputs.length > 0) {
            event.preventDefault();
            alert('Please correct the errors in the form before submitting.');
        }
    });
});
</script>
