                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navigation">
                        <ul class="navbar-nav ml-auto text-center">
                            <li class="nav-item {% if app.request.attributes.get('_route') == 'course_index' %}active{% endif %}">
                                <a class="nav-link" href="{{ path('course_index') }}">Courses</a>
                            </li>
                            <li class="nav-item {% if app.request.attributes.get('_route') == 'course_new' %}active{% endif %}">
                                <a class="nav-link" href="{{ path('course_new') }}">Create Course</a>
                            </li>
                            <li class="nav-item {% if app.request.attributes.get('_route') == 'events' %}active{% endif %}">
                                <a class="nav-link" href="{{ path('events') }}">Events</a>
